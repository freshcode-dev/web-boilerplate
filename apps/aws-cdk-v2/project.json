{
	"name": "aws-cdk-v2",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"projectType": "application",
	"sourceRoot": "apps/aws-cdk-v2/src",
	"targets": {
		"deploy": {
			"executor": "@ago-dev/nx-aws-cdk-v2:deploy",
			"options": {
				"stackName": "infra-stack",
				"requireApproval": "never",
				"noConfirmChangeset": true
			}
		},
		"destroy": {
			"executor": "@ago-dev/nx-aws-cdk-v2:destroy",
			"options": {}
		},
		"bootstrap": {
			"executor": "@ago-dev/nx-aws-cdk-v2:bootstrap",
			"options": {}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["apps/aws-cdk-v2/**/*.ts"]
			}
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "apps/aws-cdk-v2/jest.config.ts",
				"passWithNoTests": true
			},
			"configurations": {
				"ci": {
					"ci": true,
					"codeCoverage": true
				}
			}
		}
	},
	"tags": []
}
