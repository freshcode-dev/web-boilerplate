{
  "extends": ["../../.eslintrc.json"],
  "ignorePatterns": ["!**/*", "**/node_modules"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {
				"@typescript-eslint/no-unused-vars": "warn",
				"@typescript-eslint/no-explicit-any": "off"
			}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
